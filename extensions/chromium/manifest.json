{
  "manifest_version": 2,

  "name": "РосКомНадзор",
  "description": "Безопасный Интернет от РосКомНадзор с возможностью отказаться.",
  "version": "0.1",

  "permissions": [
    "proxy",
	  "storage",
    "alarms",

	  "http://*/",
	  "https://*/",
	  "ftp://*/"
  ],
